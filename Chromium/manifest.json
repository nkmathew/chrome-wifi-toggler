{
  "manifest_version": 2,
  "name": "Chrome WiFi Toggler",
  "description": "Adds an action menu button for toggling wifi on/off on Linux",
  "version": "0.1.0",

  "background": {
    "scripts": ["cwt-background.js", "chrome-api.js"]
  },
  "content_scripts": [],
  "icons": {
    "16": "images/wifi6.png",
    "48": "images/wifi6.png",
    "128": "images/wifi6.png"
  },
  "browser_action": {
    "default_icon": "images/wifi6.png"
  },
  "permissions": ["nativeMessaging"]
}
